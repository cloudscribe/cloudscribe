@if (Model.RootNode.Children.Count > 0)
{
    @:<ul class="menu">
    foreach (var node in Model.RootNode.Children)
    {

        @:<li><a href="@Url.Content(node.Value.Url)">@node.Value.Text</a></li>

    
       
    }
    @:</ul>
}

